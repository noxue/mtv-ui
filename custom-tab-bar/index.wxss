.tab-bar-fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 750rpx;
}

.tab-bg {
  z-index: 500;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 750rpx;
  height: 124rpx;
}

.tab-bar {
  z-index: 1000;
  position: relative;
  height: 124rpx;
  display: flex;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item .title {
  margin-top: 10rpx;
  font-size: 20rpx;
}

.tab-bar-item .icon {
  margin-top: 38rpx;
}

.empty-view {
  width: 100%;
  height: 0;
  height: env(safe-area-inset-bottom);
  background-color: #fff;
}
